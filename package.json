{
  "name": "smartpower",
  "version": "0.9.0",
  "author": "易采智电数据管理咨询有限公司",
  "description": "易采电力数据管理系统",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "publish": "build -p always"
  },
  "build": {
    "productName": "易采智电",
    "appId": "com.smartpower.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "artifactName": "${productName}-${os}-${version}.${ext}",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "publish": {
        "provider": "generic",
        "url": "http://www.cx8o92.cn/download/ios",
        "channel": "latest"
      }
    },
    "win": {
      "icon": "build/icons/logo.ico",
      "publish": {
        "provider": "generic",
        "url": "http://www.cx8o92.cn/download/windows",
        "channel": "latest"
      },
      "target": "nsis"
    },
    "linux": {
      "icon": "build/icons"
    },
    "nsis": {
      "oneClick": false,
      "installerLanguages": "zh-CN",
      "language": "2052",
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "area-data": "^1.0.0",
    "axios": "^0.17.1",
    "calendar-plugin": "^1.0.1",
    "clipboard": "^1.7.1",
    "countup": "^1.8.2",
    "cropperjs": "^1.2.2",
    "echarts": "^3.8.5",
    "electron-updater": "^2.19.0",
    "html2canvas": "^0.5.0-beta4",
    "iview": "^2.8.0",
    "iview-area": "^1.5.14",
    "js-cookie": "^2.2.0",
    "raphael": "^2.2.7",
    "rasterizehtml": "^1.2.4",
    "simplemde": "^1.11.2",
    "sortablejs": "^1.7.0",
    "tinymce": "^4.7.4",
    "vue": "^2.5.13",
    "vue-electron": "^1.0.6",
    "vue-kindeditor": "^0.2.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "mockjs": "^1.0.1-beta3",
    "7zip-bin-mac": "^1.0.1",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-hot-loader": "^1.3.5",
    "css-loader": "^0.28.8",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.53.4",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.14.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "happypack": "^4.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "semver": "^5.4.1",
    "style-loader": "^0.19.1",
    "unsupported": "^1.1.0",
    "url-loader": "^0.6.2",
    "vue-count-to": "^1.0.10",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.3",
    "vue-i18n": "^5.0.3",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "webpack-uglify-parallel": "^0.1.4"
  }
}
